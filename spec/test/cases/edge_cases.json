    "expected_diff": "^ {\"setkeys\":[\"id\"]}\n@ [{\"id\":1},\"name\"]\n- \"A\"\n+ \"C\"\n@ [1]\n- {\"id\":1,\"name\":\"B\"}\n",
    "expected_exit": 1,
    "compliance_level": "extended"
  },
